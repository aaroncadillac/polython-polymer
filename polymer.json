{
  "entrypoint": "index.html",
  "shell": "src/pay-app.html",
  "fragments": [
    "src/demo-page.html",
    "src/page-not-found.html",
    "src/containers/user-view.html",
    "src/containers/seller-view.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
